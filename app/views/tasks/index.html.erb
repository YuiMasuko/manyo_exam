<h1>タスク一覧</h1>
<%= link_to '新規投稿', new_task_path %>

<table>
    <tr>
      <th>タスク名</th>
      <th>内容</th>
    </tr>
    <% @tasks.each do |task| %>
      <tr>
        <td><%= task.title %></td>
        <td><%= task.content %></td>
        <td><%= link_to 'Show', task %></td>
      </tr>
    <% end %>
</table>
